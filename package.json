{
  "name": "@elizaos/plugin-claudecode",
  "description": "ElizaOS plugin for autonomous codebase modification using Claude Code SDK",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "MIT",
  "author": "ElizaOS Community",
  "keywords": [
    "plugin",
    "elizaos",
    "claude",
    "code-generation",
    "ai-coding",
    "claude-code",
    "autonomous-coding"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/plugin-claudecode"
  },
  "homepage": "https://elizaos.ai",
  "bugs": {
    "url": "https://github.com/elizaos/plugin-claudecode/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    ".npmignore",
    "package.json",
    "tsup.config.ts"
  ],
  "dependencies": {
    "@elizaos/core": "latest",
    "@anthropic-ai/claude-code": "^1.0.30",
    "zod": "3.24.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@elizaos/cli": "latest",
    "dotenv": "16.4.5",
    "prettier": "3.5.3",
    "tsup": "8.5.0",
    "typescript": "5.8.2",
    "vitest": "3.1.4"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsup",
    "lint": "prettier --write ./src",
    "test:component": "vitest run",
    "test:e2e": "elizaos test",
    "test": "npm run test:component && npm run test:e2e",
    "publish": "elizaos publish",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "zod": "3.24.2"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "ANTHROPIC_API_KEY": {
        "type": "string",
        "description": "Anthropic API key for Claude Code SDK authentication",
        "required": true
      },
      "MAX_TURNS": {
        "type": "number",
        "description": "Maximum number of turns for Claude Code operations",
        "default": 10
      },
      "PERMISSION_MODE": {
        "type": "string",
        "description": "Permission mode for file operations: 'default', 'acceptEdits', 'bypassPermissions', 'plan'",
        "default": "acceptEdits"
      }
    }
  },
  "gitHead": "d5bd5c43bfebeb7ac02f9e029f924cb6cd5c2ec7"
}